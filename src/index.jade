extends layouts/default

block prototypeHead
    a(href="components.html") Components

block content

    h1 Templates

    h2 TOC
    ul
        each t in templates
            li
                a(href= t.fileName)
                    = t.title

    h2 Team
    // This is loaded from data/data.json
    ul
        each member in team
            li= member.name

